<tr style="vertical-align: top">
    <td style="padding: 10px 10px 10px 0;">
      <% if audio_file.source_file.attached? %>
        <%= audio_file.source_file.blob.filename %>
      <% else %>
        <i>File is missing. Please delete this.</i>
      <% end %>
      <%= render "my/partial_audio_file_information", audio_file: audio_file %>
    </td>
    <td style="padding: 10px; width: 100px">
      <%= link_to(audio_file.user.name, profile_path(audio_file.user.username.presence ? audio_file.user.username : audio_file.user.id)) %>
    </td>
</tr>