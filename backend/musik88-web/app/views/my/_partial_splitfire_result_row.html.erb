<tr>
    <td style="padding: 10px 10px 10px 0;">
        <%= audio_file.source_file.blob.filename %>
    </td>
    <td style="padding: 10px; width: 130px">
        <%= link_to("<i class='icon-download'></i> Download".html_safe, rails_blob_path(audio_file.source_file, disposition: "attachment"))%>
    </td>
    <td style="padding: 10px; width: 100px">
        <%= link_to("<i class='icon-remove'></i> Delete".html_safe, my_splitfire_delete_path(audio_file.id), data: { confirm: 'Are you sure?' }, method: :delete )%>
    </td>
</tr>